{
  "name": "nestjs-structure",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:generate": "npm run typeorm migration:generate -- -d src/configurations/typeorm-configurations.service.ts",
    "typeorm:create": "npm run typeorm migration:create",
    "typeorm:run": "npm run typeorm migration:run -- -d src/configurations/typeorm-configurations.service.ts",
    "typeorm:revert": "npm run typeorm migration:revert",
    "typeorm:show": "npm run typeorm migration:show -- -d src/configurations/typeorm-configurations.service.ts",
    "typeorm:seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d configurations/typeorm-seed-configurations.service.ts",
    "seed:migrate": "npm run typeorm:seed migration:generate",
    "seed:run": "npm run typeorm:seed migration:run",
    "seed:revert": "npm run typeorm:seed migration:revert",
    "db:refresh": "npm run typeorm:cli schema:drop && npm run migration:run && npm run seed:run"
  },
  "_moduleAliases": {
    "@configs": "dist/configs",
    "@core": "dist/core",
    "@apps": "dist/apps",
    "@constants": "dist/commons/constants",
    "@decorators": "dist/commons/decorators",
    "@filters": "dist/commons/filters",
    "@guards": "dist/commons/guards",
    "@pipes": "dist/commons/pipes",
    "@utilities": "dist/commons/utilities",
    "@entities": "dist/databases/entities"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.0.3",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-flash": "^0.1.1",
    "connect-pg-simple": "^8.0.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.29.3",
    "ejs": "^3.1.8",
    "express-session": "^1.17.3",
    "fastify-swagger": "^5.1.1",
    "hbs": "^4.2.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "randomstring": "^1.2.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-pg-simple": "^7.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.6",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.0.0",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^8.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --quiet --cache --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
